<?xml version="1.0" encoding="UTF-8"?>

<Defs>  
  <RecipeDef Abstract="True" Name="SurgeryEyeStalk">
    <effectWorking>Surgery</effectWorking>
    <soundWorking>Recipe_Surgery</soundWorking>
    <workSpeedStat>MedicalOperationSpeed</workSpeedStat>
    <workSkill>Medicine</workSkill>
    <workSkillLearnFactor>16</workSkillLearnFactor>
  </RecipeDef>

  <RecipeDef Name="SurgeryFakeEyeStalk" ParentName="SurgeryEyeStalk" Abstract="True">
  <recipeUsers>
  <li>GerzeeRace</li>
  </recipeUsers>
  </RecipeDef>
  

 <RecipeDef ParentName="SurgeryFakeEyeStalk">
      <defName>InstallBionicEyeStalk</defName>
      <label>install bionic eyestalk</label>
      <description>Installs a bionic eye in the eyestalk.</description>
      <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
      <jobString>Installing bionic eyestalk.</jobString>
	  <workAmount>4500</workAmount>
    <skillRequirements>
      <Medicine>9</Medicine>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>BionicEye</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>BionicEye</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
         <li>EyeStalkLF</li>
		 <li>EyeStalkRF</li>
		 <li>EyeStalkLR</li>
		 <li>EyeStalkRR</li>
    </appliedOnFixedBodyParts>
    <addsHediff>BionicEye</addsHediff>
  </RecipeDef>
	  
	  
	  
	  
	  
	  
	  
	  
	  
	  
	
   
