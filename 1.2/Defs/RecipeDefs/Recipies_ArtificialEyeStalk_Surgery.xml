            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <categories>
            <li>Medicine</li>
         </categories>
         <thingDefs>
            <li>ArchotechEye</li>
         </thingDefs>
      </fixedIngredientFilter>
      <appliedOnFixedBodyParts>
